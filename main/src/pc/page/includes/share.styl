#share
  display none
  width 100%
  height 100%
  padding 50px
  .share-inner
    display flex
    flex-wrap nowrap
    justify-content space-between
    align-items flex-end
    position relative
    width 100%
    height 100%
  .share-buttons
    text-align center
    font-size 0
    button
      margin 0 10px
    > img
      margin-bottom 22px
      opacity 0.7